<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mauriceoegerli.uek" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Permissions">
        <param name="android-package" value="com.android.plugins.Permissions" />
    </feature>
    <name>Pictshare</name>
    <description>
        Share your photos easily
    </description>
    <author email="maurice@oeger.li" href="http://maurice.oeger.li">
        Maurice Oegerli
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon background="res/icon/android/mipmap-ldpi/ic_launcher.png" density="ldpi" foreground="res/icon/android/mipmap-ldpi/ic_launcher.png" />
    <icon background="res/icon/android/mipmap-mdpi/ic_launcher.png" density="mdpi" foreground="res/icon/android/mipmap-mdpi/ic_launcher.png" />
    <icon background="res/icon/android/mipmap-hdpi/ic_launcher.png" density="hdpi" foreground="res/icon/android/mipmap-hdpi/ic_launcher.png" />
    <icon background="res/icon/android/mipmap-xhdpi/ic_launcher.png" density="xhdpi" foreground="res/icon/android/mipmap-xhdpi/ic_launcher.png" />
    <icon background="res/icon/android/mipmap-xxhdpi/ic_launcher.png" density="xxhdpi" foreground="res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
    <icon background="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" density="xxxhdpi" foreground="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />
    <splash density="land-hdpi" src="res/icon/android/splash.png" />
    <splash density="land-ldpi" src="res/icon/android/splash.png" />
    <splash density="land-mdpi" src="res/icon/android/splash.png" />
    <splash density="land-xhdpi" src="res/icon/android/splash.png" />
    <splash density="port-hdpi" src="res/icon/android/splash.png" />
    <splash density="port-ldpi" src="res/icon/android/splash.png" />
    <splash density="port-mdpi" src="res/icon/android/splash.png" />
    <splash density="port-xhdpi" src="res/icon/android/splash.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenWidth" value="600" />
    <preference name="SplashScreenHeight" value="300" />
</widget>
